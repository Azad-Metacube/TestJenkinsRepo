    <validationRules>
        <fullName>Credit_Card_Number_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( REGEX( Credit_Card_Number__c , &quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;))</errorConditionFormula>
        <errorDisplayField>Credit_Card_Number__c</errorDisplayField>
        <errorMessage>Credit Card Number must be in this format: 9999-9999-9999-9999 or 9999999999999999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOB_validation</fullName>
        <active>true</active>
        <errorConditionFormula>DOB__c   &gt;  TODAY()</errorConditionFormula>
        <errorDisplayField>DOB__c</errorDisplayField>
        <errorMessage>DOB should not be greater then today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mix_Pct_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>OR( Mix_Pct__c  &gt; 1.0 , Mix_Pct__c  &lt; 0.0)</errorConditionFormula>
        <errorDisplayField>Mix_Pct__c</errorDisplayField>
        <errorMessage>Mix Pct must be between 0 and 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_rule</fullName>
        <active>true</active>
        <errorConditionFormula>MOD( Multiple_of_5__c ,5)   &lt;&gt; 0</errorConditionFormula>
        <errorDisplayField>Multiple_of_5__c</errorDisplayField>
        <errorMessage>Number must be a multiple of 5</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>My_Date_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>CASE(MOD( My_Date__c - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) = 0</errorConditionFormula>
        <errorDisplayField>My_Date__c</errorDisplayField>
        <errorMessage>Date must be a weekday.</errorMessage>
    </validationRules>
