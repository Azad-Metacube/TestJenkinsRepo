<apex:page standardController="Class__c">
    <apex:form >
        <apex:sectionHeader title="Class" subtitle="New Class"/>
        <apex:pageBlock >
            <apex:pageblockButtons >
                <apex:commandButton action="{!save}" value="Save"></apex:commandButton>
                <apex:commandButton action="{!cancel}" value="Cancel"></apex:commandButton>
            </apex:pageblockButtons>
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!Class__c.Name}"/>
                <apex:inputField value="{!Class__c.Name__c}"/>
                <apex:inputField value="{!Class__c.DetailedDescription__c}"/>
                <apex:inputField value="{!Class__c.Board__c}"/>
                <apex:inputField value="{!Class__c.fee__c}"/>
                <apex:inputField value="{!Class__c.MaxSize__c}"/>
                <apex:inputField value="{!Class__c.Contact__c}"/>
                <apex:inputField value="{!Class__c.MyCount__c}"/>
                
            </apex:pageBlockSection>
        </apex:pageBlock> 
    </apex:form>      
</apex:page>